{
    "name": "rcsofttech85/tic-tac-toe",
    "description": "Tic Tac Toe game using Symfony Console",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rcsofttech85\\TicTacToe\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Rcsofttech85\\TicTacToe\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "rahul",
            "email": "rcsofttech85@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.3",
        "symfony/validator": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "bin": ["bin/tic-tac"]
}
